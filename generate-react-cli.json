{
  "usesTypeScript": true,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/component/TemplateName.tsx",
        "index": "templates/component/index.ts"
      },
      "path": "src/components",
      "withIndex": true,
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "page": {
        "customTemplates": {
          "component": "templates/page/TemplateName.tsx",
          "index": "templates/page/index.ts"
        },
        "path": "src/pages",
        "withIndex": true,
        "withStyle": false,
        "withTest": false,
        "withStory": false,
        "withLazy": false
    }
  }
}